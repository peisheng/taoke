<?xml version="1.0" encoding="utf-8" ?>
<Hashs>
  <!--每个Hash节点代表一笔哈希记录-->
  <Hash>
    <!--必填-->
    <HashKey>哈希值</HashKey>
    <!--选填,当哈希值具有样本文件时填写,文件名称带有后缀-->
    <SwatchName>样本文件名称</SwatchName>
    <!--选填，样本文件的来源信息-->
    <Source>样本来源</Source>
    <!--必填,哈希类型为服务端提供，客户端定时获取更新-->
    <HashType>哈希类型</HashType>
    <!--选填,客户端采集到改样本的时间,格式：yyyy-MM-DD -->
    <CollectTime>采集时间</CollectTime>
    <!--选填-->
    <UploadUser>上报用户帐号</UploadUser>
    <!--选填-->
    <Remark>文件描述或其他备注</Remark>
  </Hash>
</Hashs>
